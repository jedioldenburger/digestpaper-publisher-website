                {% endif %}
                "worksFor": { "@id": "{{site.url}}/#org" }
              }{% unless forloop.last %},{% endunless %}
              {% endfor %}
              {% else %}
              { "@id": "{{site.url}}/#person-jedi-oldenburger" }
              {% endif %}
            ],
            "publisher": { "@id": "{{site.url}}/#org" },
            "datePublished": "{{page.date_published | default: page.date | default: site.time | date: '%Y-%m-%dT%H:%M:%S%z'}}",
            "dateModified": "{{page.date_modified | default: page.date | default: site.time | date: '%Y-%m-%dT%H:%M:%S%z'}}",
            "dateCreated": "{{page.date_created | default: page.date | default: site.time | date: '%Y-%m-%dT%H:%M:%S%z'}}",
            {% if page.article_section %}
            "articleSection": "{{page.article_section}}",
            {% else %}
            "articleSection": "{{page.categories[0] | default: 'Nieuws'}}",
            {% endif %}
            {% if page.article_body %}
            "articleBody": "{{page.article_body | strip_html | truncate: 5000}}",
            {% endif %}
            {% if page.word_count %}
            "wordCount": {{page.word_count}},
            {% endif %}
            {% if page.keywords %}
            "keywords": [
              {% for keyword in page.keywords %}
              "{{keyword}}"{% unless forloop.last %},{% endunless %}
              {% endfor %}
            ],
            {% endif %}
            {% if page.tags %}
            "about": [
              {% for tag in page.tags %}
              {
                "@type": "Thing",
                "name": "{{tag}}"
              }{% unless forloop.last %},{% endunless %}
              {% endfor %}
            ],
            {% endif %}
            "inLanguage": "{{page.lang | default: 'nl-NL'}}",
            "isAccessibleForFree": {{page.is_free | default: true}},
            "copyrightNotice": "© {{page.date | default: site.time | date: '%Y'}} DigestPaper Publishing B.V.",
            "license": "{{page.license | default: site.url}}/policies/terms-of-service/",
            "usageInfo": "{{site.url}}/policies/terms-of-service/",
            "copyrightHolder": { "@id": "{{site.url}}/#org" },
            "copyrightYear": "{{page.date | default: site.time | date: '%Y'}}",
            "genre": ["News", "Journalism", "{{page.genre | default: 'Investigation'}}"],
            {% if page.location %}
            "contentLocation": {
              "@type": "Place",
              "name": "{{page.location.name}}",
              {% if page.location.address %}
              "address": {
                "@type": "PostalAddress",
                "addressLocality": "{{page.location.city}}",
                "addressRegion": "{{page.location.region}}",
                "addressCountry": "{{page.location.country | default: 'NL'}}"
              },
              {% endif %}
              {% if page.location.coordinates %}
              "geo": {
                "@type": "GeoCoordinates",
                "latitude": {{page.location.coordinates.lat}},
                "longitude": {{page.location.coordinates.lng}}
              }
              {% endif %}
            },
            {% endif %}
            "speakable": {
              "@type": "SpeakableSpecification",
              "cssSelector": ["h1", ".article-summary", ".article-lead"],
              "xpath": [
                "/html/head/title",
                "//*[@class='article-summary']",
                "//*[@class='article-lead']"
              ]
            },
            {% if page.mentions %}
            "mentions": [
              {% for mention in page.mentions %}
              {
                "@type": "{{mention.type | default: 'Thing'}}",
                "name": "{{mention.name}}",
                {% if mention.url %}
                "url": "{{mention.url}}"
                {% endif %}
              }{% unless forloop.last %},{% endunless %}
              {% endfor %}
            ],
            {% endif %}
            {% if page.citation %}
            "citation": [
              {% for cite in page.citation %}
              {
                "@type": "CreativeWork",
                "name": "{{cite.title}}",
                {% if cite.url %}
                "url": "{{cite.url}}",
                {% endif %}
                {% if cite.author %}
                "author": "{{cite.author}}",
                {% endif %}
                {% if cite.publisher %}
                "publisher": "{{cite.publisher}}"
                {% endif %}
              }{% unless forloop.last %},{% endunless %}
              {% endfor %}
            ],
            {% endif %}
            "discussionUrl": "{{page.canonical_url | default: page.url | prepend: site.url}}#comments",
            "commentCount": {{page.comment_count | default: 0}},
            "interactionStatistic": [
              {
                "@type": "InteractionCounter",
                "interactionType": "https://schema.org/CommentAction",
                "userInteractionCount": {{page.comment_count | default: 0}}
              },
              {
                "@type": "InteractionCounter",
                "interactionType": "https://schema.org/ShareAction",
                "userInteractionCount": {{page.share_count | default: 0}}
              }
            ]
          }
          {% if page.breadcrumb %}
          ,{
            "@type": "BreadcrumbList",
            "@id": "{{page.canonical_url | default: page.url | prepend: site.url}}/#breadcrumb",
            "itemListElement": [
              {
                "@type": "ListItem",
                "position": 1,
                "name": "Home",
                "item": "{{site.url}}/"
              }
              {% for crumb in page.breadcrumb %}
              ,{
                "@type": "ListItem",
                "position": {{forloop.index | plus: 1}},
                "name": "{{crumb.name}}",
                "item": "{{crumb.url | prepend: site.url}}"
              }
              {% endfor %}
            ]
          }
          {% endif %}
        ]
      }
    </script>
